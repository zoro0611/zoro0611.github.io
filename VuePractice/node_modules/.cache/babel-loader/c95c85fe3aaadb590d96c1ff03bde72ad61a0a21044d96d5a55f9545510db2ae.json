{"ast":null,"code":"export default {\n  // props: ['name','phoneNumber','emailAddress','isFavorite'],\n  props: {\n    name: {\n      type: String,\n      required: true\n    },\n    phoneNumber: {\n      tyString\n    },\n    emailAddress: String,\n    isFavorite: String\n  },\n  data() {\n    return {\n      detailsAreVisible: false,\n      friend: {\n        id: 'manuel',\n        name: 'Manuel Lorenz',\n        phone: '0123 45678 90',\n        email: 'manuel@localhost.com'\n      },\n      friendIsFavorite: this.isFavorite //不能改變父層的值，用一個變數取代\n    };\n  },\n\n  methods: {\n    toggleDetails() {\n      this.detailsAreVisible = !this.detailsAreVisible;\n    },\n    toggleFavorite() {\n      if (this.friendIsFavorite === '1') {\n        this.friendIsFavorite = '0';\n      } else {\n        this.friendIsFavorite = '1';\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA,eAAe;EACX;EACAA,KAAK,EAAE;IACHC,IAAI,EAAE;MACFC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACd,CAAC;IACDC,WAAW,EAAE;MAACC;IAAQ,CAAC;IACvBC,YAAY,EAAEJ,MAAM;IACpBK,UAAU,EAAEL;EAChB,CAAC;EACDM,IAAI,GAAG;IACH,OAAO;MACHC,iBAAiB,EAAE,KAAK;MACxBC,MAAM,EAAE;QACJC,EAAE,EAAE,QAAQ;QACZX,IAAI,EAAE,eAAe;QACrBY,KAAK,EAAE,eAAe;QACtBC,KAAK,EAAE;MACX,CAAC;MACDC,gBAAgB,EAAE,IAAI,CAACP,UAAS,CAAE;IACtC;EACJ,CAAC;;EACDQ,OAAO,EAAC;IACJC,aAAa,GAAE;MACX,IAAI,CAACP,iBAAgB,GAAI,CAAC,IAAI,CAACA,iBAAgB;IAEnD,CAAC;IACDQ,cAAc,GAAE;MACZ,IAAG,IAAI,CAACH,gBAAe,KAAM,GAAG,EAAC;QAC7B,IAAI,CAACA,gBAAe,GAAI,GAAE;MAC9B,OACI;QACA,IAAI,CAACA,gBAAe,GAAI,GAAE;MAC9B;IACJ;EACJ;AACJ","names":["props","name","type","String","required","phoneNumber","tyString","emailAddress","isFavorite","data","detailsAreVisible","friend","id","phone","email","friendIsFavorite","methods","toggleDetails","toggleFavorite"],"sourceRoot":"","sources":["D:\\Destop\\GitHub\\zoro0611.github.io\\VuePractice\\Courses\\第7節_Component_02\\vue-cli-01-a-new-vue-project\\vue-cli-01-a-new-vue-project\\src\\components\\FriendContact.vue"],"sourcesContent":["<template>\r\n    <li>\